:syntax enable                                                                         
:colorscheme molokai

:set number relativenumber
:set nu rnu
:set showcmd
:set cursorline
:set tabstop=4
:set expandtab
:set shiftwidth=4
:set smarttab
:set softtabstop=0

:filetype indent on
:set wildmenu
:set lazyredraw

:nnoremap B ^
:nnoremap E $
:nnoremap $ <nop>
:nnoremap ^ <nop>

:nnoremap <PageUp> <nop>
:nnoremap <PageDown> <nop>
